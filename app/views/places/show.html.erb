<h1><%= @place["name"] %></h1>

<% @user = User.find_by({ "id" => session["user_id"] }) %>
<% if @user != nil %>
  <p>
  <a href="/entries/new?place_id=<%= @place["id"] %>" class="btn btn-primary">New Entry</a>
</p>

  <!-- <h2>My Entries</h2> -->
  <div class="row">
    <% for entry in @entries %>
      <div class="col-lg-4 col-sm-12">
        <% if entry["user_id"] == @user["id"]%>
          <h3><%= entry["title"] %></h3>
          <h4><%= entry["occurred_on"] %></h4>
         <img src="https://static.remove.bg/sample-gallery/graphics/bird-thumbnail.jpg"  class="img-fluid">
         <!-- "<%= entry["image"] %>"-->
          <p><%= entry["description"] %></p>
        <% end %>
      </div>
    <% end %>

<% else %>
  <p> Log in to view or create entries </p>
<% end %>



<p>
  <a href="/places">Back to All Places</a>
</p>